{
  "expo": {
    "name": "Calendars",
    "slug": "Calendars",
    "version": "1.0.0",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.calendars.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "myapp",
    "ios": {
      "bundleIdentifier": "com.calendars.app"
    }
  }
}
